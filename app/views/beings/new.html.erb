<h1>Create Being</h1>
<% if @being.errors.any? %>
<% @being.errors.full_messages.each do |msg| %>
    <p><%= msg %></p>
    <% end %>
<% end %>
<%= form_for @being do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :catchphrase %>
    <%= f.text_field :catchphrase %>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <%= f.submit %>
<% end %>
