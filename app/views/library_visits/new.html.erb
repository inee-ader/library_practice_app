<h1>Borrow a Book</h1>
<%= form_for @lib do |f| %>
    <%= f.label :location %>
    <%= f.collection_select :location, LibraryVisit.all, :location, :location %>
    <%= f.label :book %>
    <%= f.collection_select :book_id, Book.all, :id, :title %>
    <%= f.label :being %>
    <%= f.collection_select :being_id, Being.all, :id, :name %>
    <%= f.submit %>
<% end %>